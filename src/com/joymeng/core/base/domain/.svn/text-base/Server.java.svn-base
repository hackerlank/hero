package com.joymeng.core.base.domain;

/**
 * 分区服务器
 * 
 * @author mabin
 * 
 */
public class Server {
	private byte id;// 服务器id
	private int instanceId;// 服务器实例号
	private String name;// 服务器名称
	private byte type;// 服务器类型
	private byte state;// 服务器状态 0空闲， 1热闹，2拥挤，3，满，4维护，5关闭

	public byte getId() {
		return id;
	}

	public void setId(byte id) {
		this.id = id;
	}

	public int getInstanceId() {
		return instanceId;
	}

	public void setInstanceId(int instanceId) {
		this.instanceId = instanceId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public byte getType() {
		return type;
	}

	public void setType(byte type) {
		this.type = type;
	}

	public byte getState() {
		return state;
	}

	public void setState(byte state) {
		this.state = state;
	}
}
