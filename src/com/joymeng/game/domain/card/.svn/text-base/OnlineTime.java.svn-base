package com.joymeng.game.domain.card;

public class OnlineTime {

	/**
	 * 次数
	 */
	private int turns;
	
	/**
	 * 时间
	 */
	private int min;
	
	/**
	 * 增加机会
	 */
	private int chance;

	public int getTurns() {
		return turns;
	}

	public void setTurns(int turns) {
		this.turns = turns;
	}

	public int getMin() {
		return min;
	}

	public void setMin(int min) {
		this.min = min;
	}

	public int getChance() {
		return chance;
	}

	public void setChance(int chance) {
		this.chance = chance;
	}
	
	
}
