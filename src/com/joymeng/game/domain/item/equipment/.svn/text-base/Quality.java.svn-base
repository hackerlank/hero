package com.joymeng.game.domain.item.equipment;

public class Quality {

	//id 品质级别
	byte id;
	//系数
	byte qualityValue;
	//名称
	String qualityName;
	//颜色
	String qualityColor;
	/**
	 * 获取 qualityName
	 * @return the qualityName
	 */
	public String getQualityName() {
		return qualityName;
	}
	/**
	 * 设置 qualityName
	 * @param qualityName the qualityName to set
	 */
	public void setQualityName(String qualityName) {
		this.qualityName = qualityName;
	}
	/**
	 * 获取 qualityColor
	 * @return the qualityColor
	 */
	public String getQualityColor() {
		return qualityColor;
	}
	/**
	 * 设置 qualityColor
	 * @param qualityColor the qualityColor to set
	 */
	public void setQualityColor(String qualityColor) {
		this.qualityColor = qualityColor;
	}
	/**
	 * 获取 id
	 * @return the id
	 */
	public byte getId() {
		return id;
	}
	/**
	 * 设置 id
	 * @param id the id to set
	 */
	public void setId(byte id) {
		this.id = id;
	}
	/**
	 * 获取 qualityValue
	 * @return the qualityValue
	 */
	public byte getQualityValue() {
		return qualityValue;
	}
	/**
	 * 设置 qualityValue
	 * @param qualityValue the qualityValue to set
	 */
	public void setQualityValue(byte qualityValue) {
		this.qualityValue = qualityValue;
	}

}
