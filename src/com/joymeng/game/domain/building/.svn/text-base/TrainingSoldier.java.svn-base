package com.joymeng.game.domain.building;


public class TrainingSoldier {
	int id;
	short minLevel;
	short maxLevel;
	int trainingTime;
	short soldierType;
	int gold;
	int honorCount;
	/**
	 * @return GET the id
	 */
	public int getId() {
		return id;
	}
	/**
	 * @param SET id the id to set
	 */
	public void setId(int id) {
		this.id = id;
	}
	/**
	 * @return GET the minLevel
	 */
	public short getMinLevel() {
		return minLevel;
	}
	/**
	 * @param SET minLevel the minLevel to set
	 */
	public void setMinLevel(short minLevel) {
		this.minLevel = minLevel;
	}
	/**
	 * @return GET the maxLevel
	 */
	public short getMaxLevel() {
		return maxLevel;
	}
	/**
	 * @param SET maxLevel the maxLevel to set
	 */
	public void setMaxLevel(short maxLevel) {
		this.maxLevel = maxLevel;
	}
	/**
	 * @return GET the trainingTime
	 */
	public int getTrainingTime() {
		return trainingTime;
	}
	/**
	 * @param SET trainingTime the trainingTime to set
	 */
	public void setTrainingTime(int trainingTime) {
		this.trainingTime = trainingTime;
	}
	/**
	 * @return GET the soldierType
	 */
	public short getSoldierType() {
		return soldierType;
	}
	/**
	 * @param SET soldierType the soldierType to set
	 */
	public void setSoldierType(short soldierType) {
		this.soldierType = soldierType;
	}
	/**
	 * @return GET the gold
	 */
	public int getGold() {
		return gold;
	}
	/**
	 * @param SET gold the gold to set
	 */
	public void setGold(int gold) {
		this.gold = gold;
	}
	/**
	 * @return GET the honorCount
	 */
	public int getHonorCount() {
		return honorCount;
	}
	/**
	 * @param SET honorCount the honorCount to set
	 */
	public void setHonorCount(int honorCount) {
		this.honorCount = honorCount;
	}
}
